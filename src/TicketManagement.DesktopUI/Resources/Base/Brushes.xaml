<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--PRIMARY-->
    <SolidColorBrush x:Key="PrimaryHueLightBrush" Color="#FFECB3"/>
    <SolidColorBrush x:Key="PrimaryHueLightForegroundBrush" Color="#212121"/>
    <SolidColorBrush x:Key="PrimaryHueMidBrush" Color="#FFC107"/>
    <SolidColorBrush x:Key="PrimaryHueMidForegroundBrush" Color="#212121"/>
    <SolidColorBrush x:Key="PrimaryHueDarkBrush" Color="#FFA000"/>
    <SolidColorBrush x:Key="PrimaryHueDarkForegroundBrush" Color="#FFFFFF"/>
    <!--ACCENT-->
    <SolidColorBrush x:Key="SecondaryAccentBrush" Color="#00BCD4"/>
    <SolidColorBrush x:Key="SecondaryAccentForegroundBrush" Color="#757575"/>
</ResourceDictionary>